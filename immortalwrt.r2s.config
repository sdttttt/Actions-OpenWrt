# 编译优化
CONFIG_DEFAULT_TARGET_OPTIMIZATION="-O3 -pipe -mcpu=cortex-a53+crypto+crc"

# GCC
CONFIG_TOOLCHAINOPTS=y
CONFIG_GCC_USE_VERSION_13=y
