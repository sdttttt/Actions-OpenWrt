CONFIG_TARGET_rockchip=y
CONFIG_TARGET_rockchip_armv8=y
CONFIG_TARGET_rockchip_armv8_DEVICE_friendlyarm_nanopi-r2s=y
CONFIG_KERNEL_BUILD_DOMAIN="immortalwrt.org"
CONFIG_KERNEL_BUILD_USER="sdttttt"

# 编译优化
CONFIG_DEFAULT_TARGET_OPTIMIZATION="-O3 -pipe -march=armv8-a"

# all kmod
CONFIG_ALL_KMODS=y

# 开发者选项开启
CONFIG_DEVEL=y

# GCC
CONFIG_TOOLCHAINOPTS=y
CONFIG_GCC_USE_VERSION_12=y
CONFIG_USE_GC_SECTIONS=y
CONFIG_USE_LTO=y

# glibc
CONFIG_LIBC="glibc"

# Size
CONFIG_TARGET_KERNEL_PARTSIZE=64
CONFIG_TARGET_ROOTFS_PARTSIZE=960
CONFIG_TARGET_ROOTFS_SQUASHFS=y
# CONFIG_TARGET_ROOTFS_EXT4FS is not set

# extra
CONFIG_PACKAGE_luci-app-mosdns=y

CONFIG_ALL_NONSHARED=y
CONFIG_TARGET_PER_DEVICE_ROOTFS=y

CONFIG_BPF_TOOLCHAIN_BUILD_LLVM=y
CONFIG_BPF_TOOLCHAIN_HOST=y
CONFIG_USE_LLVM_PREBUILT=y
CONFIG_KERNEL_BPF_EVENTS=y
CONFIG_KERNEL_CGROUP_BPF=y
# CONFIG_BPF_TOOLCHAIN_NONE is not set

CONFIG_BUILD_LOG=y
CONFIG_COLLECT_KERNEL_DEBUG=y
CONFIG_HAS_BPF_TOOLCHAIN=y

CONFIG_TARGET_ALL_PROFILES=y
CONFIG_USE_LLVM_BUILD=y

# ipv6
CONFIG_PACKAGE_ipv6helper=y

# APP
CONFIG_PACKAGE_htop=y
CONFIG_PACKAGE_atop=y
CONFIG_PACKAGE_coremark=y
CONFIG_PACKAGE_autocore=y
CONFIG_PACKAGE_irqbalance=y
CONFIG_PACKAGE_zoneinfo-all=y
CONFIG_PACKAGE_ip-full=y
CONFIG_PACKAGE_coreutils-timeout=y
# CONFIG_PACKAGE_TURBOACC_INCLUDE_FLOW_OFFLOADING is not set
# CONFIG_PACKAGE_kmod-ipt-offload is not set
CONFIG_PACKAGE_TURBOACC_INCLUDE_SHORTCUT_FE_CM=y

# luci
CONFIG_PACKAGE_luci=y
CONFIG_LUCI_LANG_zh_Hans=y
CONFIG_PACKAGE_luci-compat=y
CONFIG_PACKAGE_default-settings-chn=y

# luci-app
CONFIG_PACKAGE_luci-app-cpufreq=y
CONFIG_PACKAGE_luci-app-attendedsysupgrade=y
CONFIG_PACKAGE_luci-app-ramfree=y
CONFIG_PACKAGE_luci-app-netdata=y
CONFIG_PACKAGE_luci-app-smartdns=y
CONFIG_PACKAGE_luci-app-autoreboot=y
