CONFIG_TARGET_rockchip=y
CONFIG_TARGET_rockchip_armv8=y
CONFIG_TARGET_MULTI_PROFILE=y

CONFIG_TARGET_DEVICE_rockchip_armv8_DEVICE_friendlyarm_nanopi-r2s=y
CONFIG_TARGET_DEVICE_rockchip_armv8_DEVICE_xunlong_orangepi-r1-plus-lts=y

# rootfs
# CONFIG_TARGET_ROOTFS_EXT4FS is not set
CONFIG_TARGET_ROOTFS_PARTSIZE=900

# kmod
CONFIG_PACKAGE_kmod-sched-bpf=y
CONFIG_PACKAGE_kmod-sched-core=y
CONFIG_PACKAGE_kmod-xdp-sockets-diag=y

#luci
CONFIG_PACKAGE_luci-app-cpufreq=y


CONFIG_PACKAGE_default-settings-chn=y

#ipv6
CONFIG_PACKAGE_ipv6helper=y

#themes
CONFIG_PACKAGE_luci-theme-argon=y
CONFIG_PACKAGE_luci-app-argon-config=y

#luci
CONFIG_PACKAGE_luci=y

CONFIG_PACKAGE_luci-app-autoreboot=y

CONFIG_PACKAGE_luci-app-diskman=y
CONFIG_PACKAGE_luci-app-diskman_INCLUDE_btrfs_progs=y
CONFIG_PACKAGE_luci-app-diskman_INCLUDE_lsblk=y

CONFIG_PACKAGE_luci-app-mosdns is not set

CONFIG_PACKAGE_luci-app-ramfree=y

CONFIG_PACKAGE_kmod-tcp-bbr=y

CONFIG_PACKAGE_TURBOACC_INCLUDE_SHORTCUT_FE_CM=y
